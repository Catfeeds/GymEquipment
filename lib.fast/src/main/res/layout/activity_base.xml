<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.saiyi.libfast.activity.view.NavBar
        android:id="@+id/title_nb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:backgroundColor="@color/nav_bgcolor"
        app:leftTextSize="@dimen/text_middle"
        app:rightTextSize="@dimen/text_middle"
        app:rightTxtColor="#FFF"
        app:titleColor="#FFF"
        app:titleSize="18dp" />

    <ViewStub
        android:id="@+id/body_vs"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>